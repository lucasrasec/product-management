{
  "version": 2,
  "builds": [
    {
      "src": "dist/server/main.js",
      "use": "@vercel/node"
    },
    {
      "src": "dist/browser/**",
      "use": "@vercel/static"
    },
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ]
}